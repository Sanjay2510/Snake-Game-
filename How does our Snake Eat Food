int f_x_pos=15;   /Creating the Food
int f_y_pos=15;
boolean gamestatus =false;
if (!gamestatus)
{
fill(255);
rect(f_x_pos*block,f_y_pos*block,block,block);
}



if(x_pos.get(0)==f_x_pos && y_pos.get(0)==f_y_pos)      /Now making our snake eat food and creating random positions for next
{
f_x_pos=(int)random(0,width);
f_y_pos=(int)random(0,height);
}


